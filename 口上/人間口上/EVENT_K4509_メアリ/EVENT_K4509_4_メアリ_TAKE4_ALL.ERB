@Mary_K4509_sexplus
;추가성경험処理
;P:0＝섹스, Z:10＝애널섹스, Z:11＝펠라치오, F:0＝메어리절정, F:1＝당신사정, F:2＝조수사정
;当関数呼び出し直前の行に, 上記変数に代入することで메어리ㆍ당신ㆍ조수にそれぞれ경험が추가されますが, 
;定型処理できないものもあるので, それらに関しては各処理箇所で경험を추가します.
;関数の最後にローカル変数초기化してるので, 추가毎に초기化することは考えなくてよろし.


;성경험추가処理
	;메어리側경험
	CALL ADDS_EXP("Ｖ경험", Z:10, TARGET)
	CALL ADDS_EXP("Ａ경험", Z:11, TARGET)
	CALL ADDS_EXP("성교경험", Z:10 + Z:11, TARGET)
	CALL ADDS_EXP("펠라경험", Z:12, TARGET)
	CALL ADDS_EXP("봉사쾌락경험", Z:12, TARGET)
	CALL ADDS_EXP("절정경험", Z:0, TARGET)
	SIF TEQUIP:MASTER:콘돔
		Z:15 = 0

;	SIF TEQUIP:ASSI:콘돔
	IF ASSI > 0
		SIF TEQUIP:ASSI:콘돔
			Z:16 = 0
	ENDIF
	CALL ADDS_EXP("질사경험", Z:15 + Z:16, TARGET)
	CALL ADDS_EXP("정액경험", Z:15 + Z:16, TARGET)

	;메어리独自경험
	;섹스実施＆당신が절정＝당신との섹스경험
	SIF Z:10 >= 1 && Z:1 >= 1
		CFLAG:810 |= 1p31
	;섹스実施＆조수が절정＝조수との섹스경험
	SIF Z:11 >= 1 && Z:2 >= 1
		CFLAG:811 |= 1p31
	;애널実施＆당신が절정＝당신との애널섹스경험
	SIF Z:11 >= 1 && Z:1 >= 1
		CFLAG:810 |= 1p41
	;애널実施＆조수が절정＝조수との애널섹스경험
	SIF Z:12 >= 1 && Z:1 >= 1
		CFLAG:811 |= 1p41
	;フェラ実施＆당신が절정＝당신との펠라경험
	SIF Z:11 >= 1 && Z:1 >= 1
		CFLAG:810 |= 1p41
	;フェラ実施＆조수が절정＝조수との펠라경험
	SIF Z:12 >= 1 && Z:1 >= 1
		CFLAG:811 |= 1p41
	;당신生사정＆섹스実施＝당신の페니스접촉 경험＆정액접촉 경험+질내사정경험
	IF Z:15 >= 1 && Z:10 >= 1
		CFLAG:810 |= 1p1
		CFLAG:810 |= 1p2
		CFLAG:810 |= 1p3
	;당신生사정＝당신の페니스접촉 경험＆정액접촉 경험
	ELSEIF Z:15 >= 1
		CFLAG:810 |= 1p1
		CFLAG:810 |= 1p2
	ENDIF
	;조수生사정＆섹스実施＝조수の페니스접촉 경험＆정액접촉 경험+질내사정경험
	IF Z:16 >= 1 && Z:10 >= 1
		CFLAG:811 |= 1p1
		CFLAG:811 |= 1p2
		CFLAG:811 |= 1p3
	;조수生사정＝조수の페니스접촉 경험＆정액접촉 경험
	ELSEIF Z:16 >= 1
		CFLAG:811 |= 1p1
		CFLAG:811 |= 1p2
	ENDIF

	;당신側경험
	CALL ADDS_EXP("사정경험", Z:1, MASTER)
	CALL ADDS_EXP("절정경험", Z:1, MASTER)
	;조수側경험
	IF ASSI > 0
		CALL ADDS_EXP("사정경험", Z:2, ASSI)
		CALL ADDS_EXP("절정경험", Z:2, ASSI)
		;ＮＴＲ기능ON＆조수で섹스を行った場合, Ｖ확장が증가
		IF FLAG:51 && Z:2 >= 1 && Z:10 >= 1
			CALL ADDS_EXP("Ｖ확장경험", Z:10, TARGET)
		ENDIF
		;ＮＴＲ기능ON＆조수で애널섹스を行った場合, Ａ확장が증가
		IF FLAG:51 && Z:2 >= 1 && Z:11 >= 1
			CALL ADDS_EXP("Ａ확장경험", Z:11, TARGET)
		ENDIF
		;수간판정
		IF (TALENT:ASSI:짐승 || TALENT:ASSI:새) && (Z:10 + Z:11 >= 1) && Z:2 >= 1
			CALL ADDS_EXP("수간경험", Z:10 + Z:11, TARGET)
			CALL ADDS_ABNORMAL_EXP( @"수간", TARGET)
		ENDIF
;	근친상간판정…は, 拾ってきたけど良く判ってないので一時保留
;	IF 近親체크(TARGET , ASSI) && Z:1 >= 1
;		CALL ADDS_ABNORMAL_EXP( @"근친상간（%RESULTS:0%）", TARGET)
;		CALL ADDS_ABNORMAL_EXP( @"근친상간（%RESULTS:1%）", ASSI)
	;ENDIF
	ENDIF

;	동성판정は당신と조수の切り分けをどーするかね…案はあるけど面倒なので取り敢えず保留.
;	IF IS_LESBIAN(TARGET,PLAYER)
;		CALL ADDS_EXP("레즈경험", Z:10 + Z:11 + Z:12, TARGET)
;		CALL ADDS_EXP("레즈경험", Z:10 + Z:11 + Z:12, PLAYER)
;	ELSEIF IS_GAY(TARGET,PLAYER)
;		CALL ADDS_EXP("ＢＬ경험", Z:10 + Z:11 + Z:12, TARGET)
;		CALL ADDS_EXP("ＢＬ경험", Z:10 + Z:11 + Z:12, PLAYER)
;	ENDIF

;불결추가처리
	IF Z:10 >= 1
		STAIN:3 |= 2
		STAIN:6 |= 2
	ENDIF
	SIF Z:11 >= 1
		STAIN:4 |= 2
	SIF Z:12 >= 1
		STAIN:0 |= 2
	IF Z:1 >= 1
		IF Z:10 >= 1
			STAIN:3 |= 4
			STAIN:6 |= 4
			STAIN:MASTER:2 |= 1
		ENDIF

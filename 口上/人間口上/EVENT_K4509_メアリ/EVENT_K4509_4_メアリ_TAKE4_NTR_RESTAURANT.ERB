;==========================================================
;@Mary_K4509_NTR_RESTO
; 식당노역에서의 NTR 처리
;<인수>
;	ARG   = 노역 캐릭터의 등록번호
;	ARG:1 = 접객수
;	ARG:2 = 고객만족도
;<반환값>
;	없음
;==========================================================
@Mary_K4509_NTR_RESTO, ARG, ARG:1, ARG:2
#LOCALSIZE 7
;LOCAL:1 = NTR 취향의 손님으로부터 유혹당할 확률
;LOCAL:2 = NTR 확정 확률보정
;LOCAL:3 = 헌팅인지 클레임인지 판정
;LOCAL:4 = 클레임 윤간에 대한 저항보정
;LOCAL:5 = 윤간인수
;LOCAL:6 = 미약 플래그
VARSET LOCAL, 0


;개별 구상 노역 ＮＴＲ 플래그가 OFF인 경우 아무것도 하지 않고 돌아온다
SIF CFLAG:TARGET:816 == 0
	RETURN
;"전속노예의목걸이"를 장비한 경우 아무것도 하지 않고 돌아온다
SIF (TEQUIP:TARGET:그외 == 1116 || TEQUIP:TARGET:그외2 == 1116 || TEQUIP:TARGET:그외3 == 1116)
	RETURN
;남자는 돌아가라
SIF TALENT:TARGET:남자 == 1
	RETURN

SIF ARG:2 == 0
	ARG:2 = 1

LOCAL:1 = LIMIT(CFLAG:TARGET:1320 * 4, 0, 40) + (TALENT:TARGET:인기 * 10) + (CFLAG:TARGET:904 / 50) + 20
LOCAL:2 = (CFLAG:TARGET:1320 * 4) + (TALENT:TARGET:즉각함락 * 10) + (CFLAG:TARGET:904 / 50)

IF BASE:TARGET:체력 >= 2000
	LOCAL:4 = 100
ELSEIF BASE:TARGET:체력 < 2000
	LOCAL:4 = 80
ELSEIF BASE:TARGET:체력 < 1000
	LOCAL:4 = 50
ELSEIF BASE:TARGET:체력 < 500
	LOCAL:4 = 0
ENDIF

;ＮＴＲ손님의 설정 처리
SELECTCASE FLAG:59
	CASE  0
		TIMES LOCAL:1 , 1.00
		TIMES LOCAL:2 , 1.00
		TIMES LOCAL:4 , 1.00
		TIMES ARG:2 , 1.50
	CASE  1
		TIMES LOCAL:1 , 1.50
		TIMES LOCAL:2 , 1.50
		TIMES LOCAL:4 , 0.80
		TIMES ARG:2 , 1.00
	CASE  2
		TIMES LOCAL:1 , 3.00
		TIMES LOCAL:2 , 3.00
		TIMES LOCAL:4 , 0.50
		TIMES ARG:2 , 0.80
ENDSELECT

SIF ASSI > 0 && NO:ASSI == CFLAG:805
	CFLAG:808 = 1

SIF ASSI > 0 && FLAG:51 && TALENT:TARGET:처녀 == 0 && CFLAG:TARGET:904 >= 250 && CFLAG:808 == 1 && RAND:10 == 0
	CFLAG:TARGET:824 = 1

LOCAL:4 += (TALENT:TARGET:반항적 + TALENT:TARGET:프라이드높음 + TALENT:TARGET:건방짐 + TALENT:TARGET:츤데레) * 5 - (TALENT:TARGET:겁쟁이 + TALENT:TARGET:얌전함 + TALENT:TARGET:프라이드낮음) * 10

	SIF (TEQUIP:TARGET:그외 == 1117 || TEQUIP:TARGET:그외2 == 1117 || TEQUIP:TARGET:그외3 == 1117)
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 뭐라고 할 수 없는 달콤한 향기를 풍기고 있다……
	;ＮＴＲ 좋아함 플래그
	;방치일수*4（상한40％）의 확률로 유혹（"인기" 소지시 +10％ , CFLAG:904의 2％ 만큼 확률 상승）
	;기본 20% 보정
	;ＮＴＲ상태인 경우는 무조건
	;조수가 찾아왔다면 (CFLAG:824가 1이면) 무조건 (10 퍼센트 확률)
	PRINTFORML
	PRINTFORML
	IF RAND:100 <= LOCAL:1 || TALENT:TARGET:ＮＴＲ > 0 || CFLAG:TARGET:824 == 1 || (TEQUIP:TARGET:그외 == 1117 || TEQUIP:TARGET:그외2 == 1117 || TEQUIP:TARGET:그외3 == 1117)
		IF CFLAG:TARGET:824 == 1
			PRINTFORML 한창 일하고 있던 %CALLNAME:TARGET%에게 반가운 얼굴이 찾아왔다.
			PRINTFORM 「앗, %CALLNAME:ASSI% 님…!
			IF RAND:2 == 0
				PRINTFORML   자, 이쪽으로 와 주세요」
			ELSE
				PRINTFORML   부디, 이쪽으로…」
			ENDIF
			IF CFLAG:823 == 0
				PRINTFORML 「이렇게 찾아와 주시다니… 어쩐 일이십니까?」
				PRINTFORML %CALLNAME:TARGET%가 이끄는 대로 자리에 앉은 %CALLNAME:ASSI%는 음식을 주문하듯 태연한 목소리로
				PRINTFORMW 알면서 뭘 물어보는 거냐고, 그 음란한 몸을 써서 어서 봉사하라고 속삭였다.
				PRINTFORML
				IF CFLAG:904 >= 1000
					PRINTFORML 「아, 네엣…%UNICODE(0x2661)%  %CALLNAME:ASSI% 님께서 원하신다면 언제든지%UNICODE(0x2661)%」
					PRINTFORML 자신을 정복한 수컷의 명령에 따라 곧바로 봉사할 준비를 하는 %CALLNAME:TARGET%.
				ELSEIF CFLAG:904 >= 750
					PRINTFORML 「우후훗…♪ 좋아요, %CALLNAME:ASSI% 님이시라면, 언제든지…%UNICODE(0x2661)%」
					PRINTFORML %CALLNAME:ASSI%의 말에 흥분해 입술을 살짝 깨물며 눈웃음을 짓는 %CALLNAME:TARGET%.
				ELSE
					PRINTFORML 「지금 말입니까…? 그, 그렇다면…  알겠습니다, 조금만 기다려 주시길…」
					PRINTFORML 조금 망설였지만, 얼굴을 붉히면서 %CALLNAME:ASSI%의 말에 따르는 %CALLNAME:TARGET%. 
				ENDIF
				CFLAG:823 = 1
			ELSE
				PRINTFORML %CALLNAME:ASSI%의 손을 잡고 자리로 이끈 %CALLNAME:TARGET%는 그 옆자리에 붙어 앉았다.
				IF CFLAG:904 >= 1000
					SELECTCASE RAND:4
					CASE 0
						PRINTFORML 「오늘도 최선을 다해 봉사하겠습니다%UNICODE(0x2661)%」
					CASE 1
						PRINTFORML 「매번 찾아주셔서 정말로 감사합니다, 오늘도 열심히 봉사할게요%UNICODE(0x2661)%」
					CASE 2
						PRINTFORML 「오늘도 잔뜩 즐겁게 해드릴게요♪」
					CASE 3
						PRINTFORML 「찾아와주신 만큼, 더욱 열심히 할게요♪」
					ENDSELECT
				ELSEIF CFLAG:904 >= 750
					IF RAND:2 == 0
						PRINTFORML 「오늘도 잔뜩 즐겁게 해드릴게요♪」
					ELSE
						PRINTFORML 「찾아와주신 만큼, 더욱 열심히 할게요♪」
					ENDIF
				ELSE
					PRINTFORML 「그럼, 그… 오늘도 봉사, 인가요…?」
					PRINTFORML 「네, 최선을 다하겠습니다…」
				ENDIF
			ENDIF
		;애ＮＴＲ
		ELSEIF TALENT:TARGET:ＮＴＲ == 1
			;위험일
			IF 위험일(TARGET) == 1
				PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 뺨을 붉히고 미소지으면서 %NTRNAME(CFLAG:TARGET:함락캐릭터)%에게 뭔가를 속삭이고 있다……
			;発情
			ELSEIF 위험일(TARGET) == 2
				PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %NTRNAME(CFLAG:TARGET:함락캐릭터)%에게 매달리듯이 몸을 붙이고 무언가를 속삭이고 있다……
			;만월発情
			ELSEIF 위험일(TARGET) == 3
				PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %NTRNAME(CFLAG:TARGET:함락캐릭터)%에게 매달리듯이 몸을 붙이고 무언가를 속삭이고 있다……
			;通常
			ELSE
				IF RAND:14 == 0
					PRINTFORMW %조사처리(CALLNAME:TARGET,"가")% %조사처리(NTRNAME(CFLAG:TARGET:함락캐릭터),"와")% 키스하고 있다……
				ELSEIF RAND:13 == 0
					PRINTFORMW %NTRNAME(CFLAG:TARGET:함락캐릭터)% 옆에서 %조사처리(CALLNAME:TARGET,"는")% 얼굴이 새빨개져 있다. 테이블 밑에서 무슨 장난을 받고 있는 것 같다……
				ELSEIF RAND:12 == 0
					PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %NTRNAME(CFLAG:TARGET:함락캐릭터)%의 옆에 앉아서, 몸을 비비듯이 밀착시키고 있다……
				ELSEIF RAND:11 == 0
					PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %NTRNAME(CFLAG:TARGET:함락캐릭터)%에게 스커트의 안쪽을 만지작거려지며 황홀해 하고 있다……
				ELSEIF RAND:10 == 0
					PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %NTRNAME(CFLAG:TARGET:함락캐릭터)%에게 어깨를 안겨 귓가에 무언가 속삭여지고 있다
				ELSEIF RAND:9 == 0
					PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %조사처리(NTRNAME(CFLAG:TARGET:함락캐릭터),"와")% 그늘에서 서로 껴안고 있다……
				ELSEIF RAND:8 == 0
					PRINTFORMW %조사처리(NTRNAME(CFLAG:TARGET:함락캐릭터),"는")% %조사처리(CALLNAME:TARGET,"를")% 옆에 앉힌 채 허리에 손을 두르고 끌어 안으며 밀착되어 있다…
				ELSEIF RAND:7 == 0
					PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %NTRNAME(CFLAG:TARGET:함락캐릭터)%에게 엉덩이를 비비어져 얼굴을 붉히고 있다……
				ELSEIF RAND:6 == 0
					PRINTFORMW %조사처리(CALLNAME:TARGET,"와")% %조사처리(NTRNAME(CFLAG:TARGET:함락캐릭터),"는")% 손가락을 엮어 가며 작은 소리로 이야기에 열중하고 있다
				ELSEIF RAND:5 == 0
					PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %조사처리(NTRNAME(CFLAG:TARGET:함락캐릭터),"와")% 손가락을 엮으면서 친근한 듯이 말하고 있다
				ELSEIF RAND:4 == 0
					PRINTFORMW %조사처리(CALLNAME:TARGET,"가")% %NTRNAME(CFLAG:TARGET:함락캐릭터)%에게 키스하고 있다……
				ELSEIF RAND:3 == 0
					PRINTFORMW 허벅지를 쓰다듬는 %조사처리(NTRNAME(CFLAG:TARGET:함락캐릭터),"를")% %조사처리(CALLNAME:TARGET,"는")% 얼굴을 붉히며 받아들이고 있다
				ELSEIF RAND:2 == 0
					PRINTFORMW 끌어 안고 키스를 요구하는 %NTRNAME(CFLAG:TARGET:함락캐릭터)%에게 %조사처리(CALLNAME:TARGET,"는")% 수줍어하면서도 혀를 섞으며 응하고 있다
				ELSE
					PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %NTRNAME(CFLAG:TARGET:함락캐릭터)%에게 어깨를 안겨 기뻐하고 있다……！
				ENDIF
			ENDIF
		;淫ＮＴＲ
		ELSEIF TALENT:TARGET:ＮＴＲ == 2
			;위험일
			IF 위험일(TARGET) == 1
				PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 손님에게 몸을 밀착시키며 무언가를 속삭이고 있다……
			;発情
			ELSEIF 위험일(TARGET) == 2
				PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 손님의 손이 자신의 치마 안에 들어오자 요염하게 미소 지었다……
			;만월発情
			ELSEIF 위험일(TARGET) == 3
				PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 손님의 손을 자신의 치마 안에 넣으면서 요염하게 미소 지었다……
			;通常
			ELSE
				PRINTFORMW %조사처리(CALLNAME:TARGET,"가")% 손님을 유혹하고 있다……
			ENDIF
		;통상
		ELSE
			;위험일 또는 발정
			IF 위험일(TARGET) == 1 || 위험일(TARGET) == 2 || 위험일(TARGET) == 3
				SELECTCASE RAND:5
				CASE 0
					PRINTFORML %조사처리(CALLNAME:TARGET,"는")% 이전에 몸을 섞었던 단골손님에게 말을 걸고 있다……
					PRINTFORML 손님과의 섹스가 마음에 들었던 것을 떠올린 그녀는 남들 몰래 팬티를 벗었다……
					PRINTFORMW 그녀는 촉촉하게 젖은 팬티를 손님에게 건네며, "1회 무료 이용권"이라고 속삭였다……!
					TEQUIP:98 = 6
				CASE 1
					PRINTFORML %조사처리(CALLNAME:TARGET,"는")% 살짝 달뜬 얼굴로 계산을 돕고 있다……
					PRINTFORML 계산하려는 사람이 이전에 몸을 섞었던 단골손님인 것을 알아보고 입맛을 다시고 있다……
					PRINTFORML 그녀는 영수증이 필요한지 물어보고, 스탬프를 찍어주고 나서, 마지막에 카드를 돌려줄 때
					PRINTFORMW 슬쩍 몸을 밀착하며, "디저트"를 준비해 놓았다고 손님에게 속삭였다……!
				CASE 2
					PRINTFORML 성욕 때문에 감각이 예민해진 %조사처리(CALLNAME:TARGET,"는")% 자신을 향하는 강렬한 시선을 눈치챘다……
					PRINTFORML 이전에 몸을 섞었던 단골손님이 노골적인 시선으로 그녀의 몸을 구석구석 핥듯이 감상하고 있다……
					PRINTFORML 손님의 자지가 상당히 컸던 것을 떠올린 그녀는 다른 사람들의 시선이 뜸해진 틈을 타 손님을 향해 고개를 돌리고,
					PRINTFORMW 손가락을 둥글게 말아 입가에 가져다 대고 앞뒤로 움직이며, 손가락 구멍 안쪽으로 혀를 날름거리는 모습을 보여주었다……!
				CASE 3
					PRINTFORM %조사처리(CALLNAME:TARGET,"는")%
					SELECTCASE RAND:3
					CASE 0
						PRINTFORM  체격이 큰
					CASE 1
						PRINTFORM  잘생긴
					CASE 2
						PRINTFORM  돈이 많아 보이는
					ENDSELECT
					PRINTFORML  손님의 말상대를 하고 있다……
					PRINTFORML 손님이 은근슬쩍 치마 속으로 손을 넣으려고 하자, 몸이 달아오른 그녀는 그에 응해 손님의 사타구니로 손을 가져갔다……
					PRINTFORMW 바지 아래로도 느껴지는 상당한 크기에 만족하며 입맛을 다신 그녀는 손님의 귀에 무언가를 속삭이고 있다……!
				CASE 4
					PRINTFORML %조사처리(CALLNAME:TARGET,"는")% 물을 가져다 달라는 손님의 요청에 발걸음을 옮겼다……
					PRINTFORML 테이블에 다가가자, 앉아있는 사람은 이전에 몸을 섞었던 손님이었다……
					PRINTFORML 손님과의 섹스가 만족스러웠던 것을 떠올린 그녀는 욕구를 억누르지 않았다……
					PRINTFORMW 그녀는 시선이 없는 틈을 타, 입에 물을 머금고 손님에게 키스해 혀를 섞으며 입으로 물을 먹여주었다……!
				ENDSELECT
			;통상

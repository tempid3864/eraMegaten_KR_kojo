IF LOCAL:3 == 1
	CFLAG:TARGET:질내사정횟수 = 1
	CFLAG:TARGET:본방횟수 = 1
	LOCAL:1 = 2 + RAND:4
	LOCAL:2 = LOCAL:1 + RAND:(LOCAL:1)
	TCVAR:TARGET:획득바람경험 = 1

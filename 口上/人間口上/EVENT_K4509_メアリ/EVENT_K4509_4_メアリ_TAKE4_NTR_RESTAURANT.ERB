ELSEIF LOCAL:3 == 2
	CFLAG:TARGET:펠라횟수 = 1 + RAND:3
	TCVAR:TARGET:획득바람경험 = 1
;ＮＴＲ 좋아하는 손님（본방）
ELSEIF LOCAL:3 == 3
	ARG:1 = -2
	CFLAG:TARGET:질내사정횟수 = 1
	CFLAG:TARGET:본방횟수 = 1
	LOCAL:1 = 2 + RAND:4
	LOCAL:2 = LOCAL:1 + RAND:(LOCAL:1)
